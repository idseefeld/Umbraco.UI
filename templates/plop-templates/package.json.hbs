{
  "name": "@umbraco-ui/{{> tagnamePartial }}",
  "version": "0.0.0",
  "license": "MIT",
  "keywords": [
    "Umbraco",
    "Custom elements",
    "Web components",
    "UI",
    "Lit",
    "{{ displayName name }}"
  ],
  "description": "Umbraco UI {{ name }} component",
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git",
    "directory": "packages/{{> tagnamePartial }}"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "main": "./dist/{{> tagnamePartial }}.min.js",
  "module": "./lib/index.js",
  "type": "module",
  "customElements": "custom-elements.json",
  "exports": {
    ".": "./dist/index.js",
    "./define": "./define/index.js"
  },
  "directories": {
    "dist": "./dist",
    "define": "./define"
  },
  "files": [
    "custom-elements.json"
  ],
  "dependencies": {
    "@umbraco-ui/uui-base": "{{uuiBaseVersion}}"
  },
  "scripts": {
    "build": "npm run analyze && tsc --build --force && rollup -c rollup.config.js",
    "clean": "tsc --build --clean && rimraf dist custom-elements.json",
    "analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://uui.umbraco.com/?path=/story/{{> tagnamePartial }}"
}