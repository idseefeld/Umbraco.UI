{
  "name": "@umbraco-ui/uui-button",
  "version": "0.1.7",
  "license": "MIT",
  "description": "All-round button, Umbraco backoffice style.",
  "keywords": [
    "Umbraco",
    "Custom elements",
    "Web components",
    "UI",
    "Lit",
    "Button"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git",
    "directory": "packages/uui-button"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "main": "./dist/uui-button.min.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "type": "module",
  "customElements": "custom-elements.json",
  "exports": {
    ".": "./lib/index.js",
    "./define": "./define/index.js"
  },
  "directories": {
    "dist": "dist",
    "define": "define"
  },
  "files": [
    "define",
    "dist",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "custom-elements.json"
  ],
  "dependencies": {
    "@umbraco-ui/uui-base": "0.0.14"
  },
  "peerDependencies": {
    "@umbraco-ui/uui-icon-registry-essential": "0.0.0"
  },
  "scripts": {
    "build": "npm run analyze && tsc --build --force && rollup -c rollup.config.js",
    "clean": "tsc --build --clean && rimraf dist lib/*.js lib/**/*.js lib/**/*.d.ts custom-elements.json",
    "analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://uui.umbraco.com/?path=/story/uui-button"
}
