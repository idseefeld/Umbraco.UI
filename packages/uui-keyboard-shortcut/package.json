{
  "name": "@umbraco-ui/uui-keyboard-shortcut",
  "version": "0.0.10",
  "license": "MIT",
  "keywords": [
    "Umbraco",
    "Custom elements",
    "Web components",
    "UI",
    "Lit",
    "Keyboard shortcuts"
  ],
  "description": "This package contains two web components - uui-key and uui-keyboard-shortcut. They can be used to illustrate key combination for keyboard shortcuts",
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git",
    "directory": "packages/uui-keyboard-shortcut"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "main": "./dist/uui-keyboard-shortcut.min.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "customElements": "custom-elements.json",
  "directories": {
    "dist": "./dist",
    "define": "./define"
  },
  "exports": {
    ".": "./dist/index.js",
    "./define": "./define/index.js"
  },
  "files": [
    "custom-elements.json"
  ],
  "dependencies": {
    "@umbraco-ui/uui-base": "0.0.14"
  },
  "scripts": {
    "build": "npm run analyze && tsc --build --force && rollup -c rollup.config.js",
    "clean": "tsc --build --clean && rimraf dist custom-elements.json",
    "analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
