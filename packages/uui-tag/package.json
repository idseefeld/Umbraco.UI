{
  "name": "@umbraco-ui/uui-tag",
  "version": "0.0.16",
  "license": "MIT",
  "keywords": [
    "Umbraco",
    "Custom elements",
    "Web components",
    "UI",
    "Lit",
    "Tag",
    "Tags"
  ],
  "description": "Tag component from Umbraco UI components library. Comes in one shape, but different looks and sizes",
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git",
    "directory": "packages/tag"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "main": "./dist/uui-tag.min.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "customElements": "custom-elements.json",
  "directories": {
    "dist": "./dist",
    "define": "./define"
  },
  "exports": {
    ".": "./dist/index.js",
    "./define": "./define/index.js"
  },
  "files": [
    "custom-elements.json"
  ],
  "dependencies": {
    "@umbraco-ui/uui-base": "0.0.14"
  },
  "scripts": {
    "build": "npm run analyze && tsc --build --force && rollup -c rollup.config.js",
    "clean": "tsc --build --clean && rimraf dist custom-elements.json",
    "analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://uui.umbraco.com/?path=/story/uui-tag"
}
